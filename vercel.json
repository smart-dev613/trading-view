{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    }
}
