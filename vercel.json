{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/src/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build --prefix frontend",
  "devCommand": "npm run dev --prefix frontend"
}